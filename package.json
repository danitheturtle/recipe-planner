{
  "name": "recipe-planner",
  "packageManager": "npm@11.6.0",
  "version": "0.0.1",
  "license": "none",
  "type": "module",
  "scripts": {
    "update": "npm install && cd src-tauri && cargo update",
    "start": "tauri dev",
    "build": "tauri build",
    "js:build": "vite --config ./src-js/vite.config.ts --emptyOutDir build",
    "dev": "vite --config ./src-js/vite.config.ts --mode development",
    "test": "vitest --config ./src-js/vitest.config.ts watch",
    "js:format": "eslint --config ./src-js/eslint.config.js --no-warn-ignored ./src-js/* --fix"
  },
  "dependencies": {
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-router": "~7.9.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/cli": "^2.8.4",
    "@testing-library/react": "~16.3.0",
    "@types/node": "^24.5.2",
    "@types/react": "~19.1.13",
    "@types/react-dom": "~19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "~5.2.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "postcss": "~8.5.2",
    "postcss-input-range": "^6.0.0",
    "postcss-preset-env": "~10.4.0",
    "prettier": "~3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "~7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~3.2.4"
  },
  "engines": {
    "node": "~22.19.0",
    "nvm": "~0.40.3"
  }
}
